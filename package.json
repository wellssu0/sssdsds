    "webpack-dev-server": "3.11.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpackbar": "^5.0.0-3",
    "workbox-webpack-plugin": "6.0.0-alpha.3"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 11"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "setupFiles": [
      "raf/polyfill"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "setupFilesAfterEnv": [
      "<rootDir>src/setupTests.ts"
    ],
    "snapshotSerializers": [
      "enzyme-to-json"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|svg|jpg|jpeg|png|gif)$": "identity-obj-proxy",
      "^@constants/(.*)$": "<rootDir>/src/constants//$1",
      "^@services/(.*)$": "<rootDir>/src/services//$1",
      "^@utils/(.*)$": "<rootDir>/src/utils//$1",
      "^@assets/(.*)$": "<rootDir>/src/assets//$1",
      "^@components/(.*)$": "<rootDir>/src/components//$1",
      "^@views/(.*)$": "<rootDir>/src/containers/views//$1",
      "^@layouts/(.*)$": "<rootDir>/src/containers/layouts//$1",
      "^@shared/(.*)$": "<rootDir>/src/containers/shared//$1",
      "^@router/(.*)$": "<rootDir>/src/router//$1"
    }
  }
}
